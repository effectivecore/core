        # field 'Digit' (number)
        - number|Field_Number
            title: Digit
            description|Text
              text: Field "%%_title".
              args
              - title: Field_Number
            element_attributes
            - name: number
            - min: -0.1
            - max: +0.1
            - step: 0.000002
            - value: 0
        # field 'Range'
        - range|Field_Range
            title: Range
            description|Text
              text: Field "%%_title".
              args
              - title: Field_Range
            element_attributes
            - name: range
            - min: -1
            - max: +1
            - step: 0.2
            - value: 0
        # field 'Color'
        - color|Field_Color
            title: Color
            description|Text_multiline
              text
              - 0: Field "%%_title".
              - 1: Color should be in the format "#abcdef", where "ab" is the value of the red component, "cd" — green and "ef" — blue.
              args
              - title: Field_Color
            element_attributes
            - name: color
            - value: #ffffff
        # ─────────────────────────────────────────────────────────────────────
        # date/time
        # ─────────────────────────────────────────────────────────────────────
        - time|Field_Time
            title: Time
            description|Text_multiline
              text
              - 0: Field "%%_title".
              - 1: If the "value_current_if_null" parameter is "true" and the initial value is not specified, the current value will be used.
              - 2: If the "step" attribute is "60" then the ":00" seconds will be truncated.
              args
              - title: Field_Time
            element_attributes
            - name: time
            - step: 1
            - value: 01:23:45
#           - min: 00:00:00
#           - max: 23:59:59
            value_current_if_null: true
        # field 'Date'
        - date|Field_Date
