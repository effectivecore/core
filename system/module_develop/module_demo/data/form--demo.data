        # field 'File'
        - file_text|Field_File
            title: Text
            item_title: Text
            upload_dir: demo/
            description|Text
              text: Field "%%_title".
              args
              - title: Field_File
            element_attributes
            - name: file_text
            is_debug_mode: false
        # field 'Pictures'
        - file_picture|Field_File_picture
            title: Pictures
            min_files_number: 0
            max_files_number: 3
            upload_dir: demo/
            description|Text
              text: Field "%%_title".
              args
              - title: Field_File_picture
            element_attributes
            - name: file_picture[]
            - multiple: true
            thumbnails
            - =: small
            is_debug_mode: true
        # field 'Audio'
        - file_audio|Field_File_audio
            title: Audio
            upload_dir: demo/
            audio_player_on_manage_is_visible: true
            description|Text
              text: Field "%%_title".
              args
              - title: Field_File_audio
            element_attributes
            - name: file_audio[]
            is_debug_mode: false
        # field 'Video'
        - file_video|Field_File_video
            title: Video
            upload_dir: demo/
            description|Text
              text: Field "%%_title".
              args
              - title: Field_File_video
            element_attributes
            - name: file_video[]
            is_debug_mode: false
        # widget 'Files'
        - files_texts|Widget_Files
            title: Texts
            item_title: Text
            state: opened
            group_name: files_texts
            upload_dir: demo/
            types_allowed
            - =: txt
        # widget 'Pictures'
        - files_pictures|Widget_Files_pictures
            title: Pictures
            state: opened
            group_name: files_pictures
            upload_dir: demo/
            types_allowed
            - =: jpg
            - =: jpeg
            - =: png
            - =: gif
            - =: svg
            thumbnails_is_allowed: true
            thumbnails
            - =: small
        # widget 'Audios'
        - files_audios|Widget_Files_audios
            title: Audios
            state: opened
            group_name: files_audios
            upload_dir: demo/
            audio_player_on_manage_is_visible: true
            cover_is_allowed: true
            types_allowed
            - =: mp3
        # widget 'Videos'
        - files_videos|Widget_Files_videos
            title: Videos
            state: opened
            group_name: files_videos
            upload_dir: demo/
            poster_is_allowed: true
            types_allowed
            - =: mp4
        # field 'Text'
        - text|Field_Text
            title: Text
            description_state: opened
            description|Text_multiline
              text
              - 0: Field "%%_title".
              - 1: This description is always shown.
              args
              - title: Field_Text
            element_attributes
            - name: text
            - value: text in input
            - minlength: 5
            - pattern: ^[a-zA-Z0-9 ]+$
            - data-is-wide-field: true
        # field 'Search'
        - search|Field_Search
            title: Search
            description|Text
              text: Field "%%_title".
              args
              - title: Field_Search
            element_attributes
            - name: search
            - value: text in search
            - data-is-wide-field: true
        # field 'URL'
        - url|Field_URL
            title: URL
            description|Text
              text: Field "%%_title".
              args
              - title: Field_URL
            element_attributes
            - name: url
            - value: http://domain/path?q=w#p1
            - data-is-wide-field: true
            is_trim_trailing_slash: false
            is_trim_trailing_question: false
            is_trim_trailing_sharp: false
            is_allowed_unicode: true
            scope: absolute
            parts
            - protocol: +
            - domain: +
            - path: +
            - query: +
            - anchor: +
#           scope: relative
#           parts
#           - path: +
#           - query: +
#           - anchor: +
        # field 'Email'
        - email|Field_Email
            title: EMail address
            description|Text
              text: Field "%%_title".
              args
              - title: Field_Email
            element_attributes
            - name: email
            - value: test1@example.com,test2@example.com
            - multiple: true
            - data-is-wide-field: true
        # field 'Telephone number'
        - tel|Field_Tel
            title: Telephone number
            description|Text_multiline
              text
              - 0: Field "%%_title".
              - 1: Field value should be represented in the international format of telephone numbers.
              args
              - title: Field_Tel
            element_attributes
            - name: tel
            - value: &plus;000112334455
        # field 'Nickname'
        - nickname|Field_Nickname
            title: Nickname
            description|Text
              text: Field "%%_title".
              args
              - title: Field_Nickname
            element_attributes
            - name: nickname
            - value: user
        # field 'Password'
        - password|Field_Password
            title: Password
            description_position: top
            description|Text_multiline
              text
              - 0: Field "%%_title".
