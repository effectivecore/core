- demo_selection_field_types|Selection
    id: demo_selection_field_types
    main_entity_name: demo_data
    title: Demonstration of field types for Selection
    decorator_settings
    - view_type: ul
    query_settings
    - conditions
      - conjunction_!and
        - nicknames
          - nickname_!f: nickname
          - nickname_operator: =
          - nickname_!v: user_001
        - email
          - email_!f: ~demo_join.email
          - email_operator: =
          - email_!v: user_001@example.com
    - order
      - title_!f: id
      - title_type: ASC
    - limit: 1
    fields
    - checkboxes
      - type_checkbox
          title|Text
            text: Type "%%_type"
            args
            - type: checkbox
          settings
          - name: is_checked[]
          weight: +400
    - main
      - type_field
          entity_field_name: id
          title|Text
            text: Type "%%_type"
            args
            - type: field
          weight: +380
    - join
      - demo_join
          type: left outer join
          entity_name: demo_join
          entity_field_name: id_data
          on_entity_name: demo_data
          on_entity_field_name: id
          fields
          - email
              entity_field_name: email
              title|Text
                text: Type "%%_type"
                args
                - type: join field
              weight: +360
    - texts
      - type_text_with_translation
          title|Text
            text: Type "%%_type"
            args
            - type: text + translation
          text: text with translation
          is_trimmed: true
          is_apply_translation: true
          weight: +340
      - type_text_with_translation_with_token
          title|Text
            text: Type "%%_type"
            args
            - type: text + translation + token
          text: text with translation and token demo_text = "%%_demo_text"
          is_trimmed: true
          is_apply_translation: true
          is_apply_tokens: true
          weight: +320
      - type_text_with_token
          title|Text
            text: Type "%%_type"
            args
            - type: text + token
          text: text with token demo_text = "%%_demo_text"
          is_trimmed: true
          is_apply_tokens: true
          weight: +300
    - markup
      - type_markup
          title|Text
            text: Type "%%_type"
            args
            - type: markup
          markup|Markup
            tag_name: span
            children
            - data|Text
                text: markup
          weight: +280
    - handlers
      - type_handler
          title|Text
            text: Type "%%_type"
            args
            - type: handler
          settings
          - demo_value: value from handler settings
          handler: \effcore\modules\demo\Events_Selection::demo_handler
          weight: +260
