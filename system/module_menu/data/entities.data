entities
- tree|Entity
    name: tree
    title: Tree
    title_plural: Trees
    table_name: trees
    managing_is_enabled: true
    managing_group_id: menu
    has_parallel_checking: true
    with_is_embedded: true
    with_module_id: true
    with_data: true
    access_select
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree__select
    access_insert
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree__insert
    access_update
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree__update
    access_delete
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree__delete
    fields
    - id
        title: ID
        type: varchar
        size: 255
        collate: nocase
        not_null: true
        check: (id <> '')
        managing
          is_enabled_on_insert: true
          is_enabled_on_update: true
          control
            class: \effcore\Field_ID_text
            properties
            - is_validate_uniqueness: true
            - weight: +400
            element_attributes_on_update
            - disabled: true
    - title
        title: Title
        type: varchar
        size: 1024
        collate: nocase
        default: null
        converters
          on_insert: \effcore\Core::html_entity_encode
          on_update: \effcore\Core::html_entity_encode
        managing
          is_enabled_on_insert: true
          is_enabled_on_update: true
          control
            class: \effcore\Field_Text
            properties
            - weight: +380
            element_attributes
            - maxlength: 1024
            - size: 60
    - title_is_visible
        title: Title is visible
        type: boolean
        not_null: true
        default: 1
        managing
          is_enabled_on_insert: true
          is_enabled_on_update: true
          control
            class: \effcore\Field_Select_logic
            properties
            - weight: +360
            properties_on_insert
            - selected
              - =: 1
    - attributes
        title: Attributes
        type: blob
        default: null
        converters
          on_select: unserialize
          on_insert: \effcore\Core::data_serialize
          on_update: \effcore\Core::data_serialize
          on_render: \effcore\Widget_Attributes::value_to_markup
        managing
          is_enabled_on_insert: true
          is_enabled_on_update: true
          control
            class: \effcore\Widget_Attributes
            properties
            - name_complex: attributes
            - weight: +340
    - created
        title: Created
        type: datetime
        default: null
        check: (created <> '')
        managing
          control
            class: \effcore\Field_DateTime_local
            properties
            - weight: +320
    - updated
        title: Updated
        type: datetime
        default: null
        check: (updated <> '')
        managing
          control
            class: \effcore\Field_DateTime_local
            properties
            - weight: +300
    - access
        title: Read access
        type: blob
        default: null
        converters
          on_select: unserialize
          on_insert: \effcore\Core::data_serialize
          on_update: \effcore\Core::data_serialize
          on_render: \effcore\Widget_Access::value_to_markup
        managing
          is_enabled_on_insert: true
          is_enabled_on_update: true
          control
            class: \effcore\Widget_Access
            properties
            - weight: +280
    constraints
    - primary_id
        type: primary
        fields
        - =: id
    indexes
    - index_created
        type: index
        fields
        - =: created
    - index_updated
        type: index
        fields
        - =: updated
