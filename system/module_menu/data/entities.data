    - foreign_id_tree
        type: foreign
        fields
        - =: id_tree
        references: trees
        references_fields
        - =: id
    - foreign_id_parent
        type: foreign
        fields
        - =: id_parent
        references: tree_items
        references_fields
        - =: id
    indexes
    - index_weight
        type: index
        fields
        - =: weight
    - index_created
        type: index
        fields
        - =: created
    - index_updated
        type: index
        fields
