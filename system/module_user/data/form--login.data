forms
- login|Form
    attributes
    - id: login
    - method: post
    - novalidate: true
    children
    - credentials|Fieldset
        children
        - email|Field_Email
        - password|Field_Password
        - session_params|Group_Switchers
            element_attributes
            - name: session_params[]
            items
            - is_long_session: long session
            - is_fixed_ip: bind to my IP address
            checked
            - =: is_long_session
            - =: is_fixed_ip
        - captcha|Form_plugin
            name: field_captcha
    - button_login|Button
        title: login
        attributes
        - value: login
        weight: -500