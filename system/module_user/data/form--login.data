forms
- login|form
    attributes
    - id: login
    - method: post
    - novalidate: true
    children
    - credentials|fieldset
        title: Credentials
        children
        - email|field_email
        - password|field_password
        - session_params|group_switchers
            element_attributes
            - name: session_params[]
            values
            - is_remember: remember me
            - is_fixed_ip: on this ip
            checked
            - =: is_remember
            - =: is_fixed_ip
        - captcha|pluggable_class
            name: field_captcha
    - button_login|button
        title: login
        attributes
        - value: login