forms
- registration|Form
    attributes
    - id: registration
    - method: post
    - novalidate: true
    children
    - credentials|Fieldset
        children
        - email|Field_Email
            entity_name: user
            entity_field_name: email
            is_validate_uniqueness: true
        - nickname|Field_Nickname
            entity_name: user
            entity_field_name: nickname
            is_validate_uniqueness: true
        - password|Field_Password
        - timezone|Field_Select_timezone
            element_attributes
            - data-source: uagent-timezone
        - session_params|Group_Switchers
            element_attributes
            - name: session_params[]
            items
            - is_long_session: long session
            - is_fixed_ip: bind to my IP address
            checked
            - =: is_long_session
            - =: is_fixed_ip
        - captcha|Form_plugin
            name: field_captcha
    - button_register|Button
        title: register
        attributes
        - value: register
        weight: -500