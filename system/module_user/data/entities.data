    fields
    - id
        title: ID
        type: autoincrement
        not_null: true
        managing
          is_enabled_on_insert: true
          is_enabled_on_update: true
          control
            class: \effcore\Field_ID_number
            properties
            - weight: +400
            element_attributes
            - disabled: true
    - nickname
        title: Nickname
        type: varchar
        size: 32
        collate: nocase
        not_null: true
        check: (nickname <> '')
        managing
          is_enabled_on_insert: true
          is_enabled_on_update: true
          control
            class: \effcore\Field_Nickname
            properties
            - is_validate_uniqueness: true
            - weight: +380
    - email
        title: EMail address
        type: varchar
        size: 64
        collate: nocase
        not_null: true
        check: (email <> '')
        managing
          is_enabled_on_insert: true
          is_enabled_on_update: true
          control
            class: \effcore\Field_Email
            properties
            - is_validate_uniqueness: true
            - weight: +360
    - avatar_path
        title: Avatar path
        type: varchar
        size: 255
        collate: nocase
        default: null
        managing
          is_enabled_on_insert: true
          is_enabled_on_update: true
          control
            class: \effcore\Field_File_picture
            properties
            - title: Avatar
            - max_file_size: 30K
            - upload_dir: avatars/
            - fixed_name: avatar-%%_user_id
            - thumbnails
              - =: small
            - weight: +340
            element_attributes_on_insert
            - disabled: true
    - timezone
        title: Time zone
        type: varchar
        size: 64
        collate: nocase
        default: UTC
        converters
          on_render: \effcore\Field_Select_timezone::value_to_markup
